{
    "name": "inkdex.github.io",
    "description": "The Inkdex website.",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "eslint --fix",
        "lint:check": "eslint",
        "format": "prettier --write '**/*{.md,.yml,.yaml,.json,.js,.ts}'",
        "format:check": "prettier --check '**/*{.md,.yml,.yaml,.json,.js,.ts}'",
        "tsc": "tsc --project tsconfig.json --noEmit",
        "build": "vitepress build src",
        "preview": "vitepress preview src",
        "dev": "vitepress dev src",
        "test": "npm run tsc && npm run lint:check && npm run format:check && npm run build",
        "prepare": "husky"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
        "@types/eslint": "^9.6.1",
        "eslint": "^9.39.1",
        "husky": "^9.1.7",
        "prettier": "^3.6.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.4",
        "vitepress": "^1.6.4",
        "vue": "^3.5.24"
    }
}
